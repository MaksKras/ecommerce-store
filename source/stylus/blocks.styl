.clear
    clear both

.b-common-wrap
    padding-bottom 100px

.b-container
    max-width 1210px
    min-width 980px
    width 100%
    margin 0 auto
    position relative
    height 100%
    padding 0 30px

.b-header-line
    background-color #2e7fd8
    height 42px
    
    &__wrap
        display table
        width 100%
        height 100%
        font-size 0
        
    &__left
        width 1px
        white-space nowrap
        
    &__right
        text-align right
        
    &__left, &__right
        display table-cell
        vertical-align middle
        
    &__text
        color #fff
        font-size 13px
        font-weight 500
        text-decoration none
        display block
        transition color .2s
        &:hover
            color darken(#fff, 10)
            
    &__item
        display inline-block
        vertical-align middle
        padding 0 20px
        border-right 1px solid alpha(#fff, .1)
        
        &:first-child
            padding-left 0
            
        &:last-child
            padding-right 0
            border-right 0

.b-dropdown-switcher
