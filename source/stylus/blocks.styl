    font-size 11px
    font-weight 500
    text-transform uppercase
    text-decoration none
    padding 6px 9px
    background-color #2872c2
    border-radius 2px
    transition background-color .2s
    &:hover
        background-color darken(#2872c2, 5)

.b-header-content
