            display inline-block
            vertical-align middle
            margin-left 2px
            sprite($s-chev-b)

.b-icon-item
    color #fff
    font-size 13px
    font-weight 500
    text-decoration none
    &:before
        content ''
        display inline-block
        vertical-align middle
        margin-right 4px
        
    &_heart
        &:before
            sprite($s-heart)

.b-btn
